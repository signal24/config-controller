{
    "name": "@signal24/config-controller",
    "version": "1.1.0",
    "description": "Simple Kubernetes controller to create key/value Secrets from encrypted .env files stored in ConfigMaps",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "dev": "ts-node src/index.ts",
        "build": "rm -rf dist && tsc -p tsconfig.json",
        "format": "prettier --write . && eslint src --rule 'prettier/prettier: off' --fix",
        "test": "jest",
        "watch": "tsc -w -p tsconfig.json"
    },
    "author": "Sean Ferguson <hello@sgnl24.com>",
    "license": "ISC",
    "dependencies": {
        "@kubernetes/client-node": "^1.4.0",
        "@signal24/config": "1.9.2",
        "pino": "^10.1.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/jest": "^30.0.0",
        "@types/node": "^25.0.3",
        "buildkite-test-collector": "^1.9.3",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-unused-imports": "^4.3.0",
        "jest": "^30.2.0",
        "prettier": "^3.7.4",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.2",
        "tslib": "^2.8.1",
        "type-fest": "^5.3.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.51.0"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "packageManager": "yarn@4.9.1"
}
